{
  "compilerOptions": {
    "target": "ES2016",
    "module": "Node16",
    "outDir": "dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node16"
  },
  "include": [
    "server.ts",
    "cypress.config.ts"
  ],
  "exclude": [
    "node_modules",
    "src"
  ]
}
